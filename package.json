    "@types/pdfkit": "0.7.36",
    "@types/qrcode": "1.3.4",
    "@types/sjcl": "1.0.28",
    "@types/uuid": "3.4.4",
    "base64-stream": "1.0.0",
    "bgoldjs-lib": "3.3.3",
    "bignumber.js": "8.0.1",
    "bip69": "2.1.4",
    "bitcoinjs-lib": "3.3.2",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-bignumber": "3.0.0",
