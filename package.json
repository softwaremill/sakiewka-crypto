{
  "name": "sakiewka-crypto",
  "version": "1.0.0",
  "description": "Client-side Sakiewka app",
  "main": "dist/index.js",
  "engines": {
    "node": ">=10.0.0 <11.0.0",
    "npm": ">=6.0.0 <7.0.0 "
  },
  "scripts": {
    "postinstall": "tsc --skipLibCheck",
    "test": "jest --forceExit --verbose false --detectOpenHandles",
    "watch-test": "npm run test -- --watchAll",
    "build": "tsc",
    "watch": "tsc -w",
    "generate-docs": "typedoc --options ./typedoc.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/softwaremill/sakiewka-client.git"
  },
  "author": "SoftwareMill",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/softwaremill/sakiewka-client/issues"
  },
  "homepage": "https://github.com/softwaremill/sakiewka-client#readme",
  "devDependencies": {
    "concurrently": "4.1.2",
    "husky": "3.0.8",
    "jest": "24.9.0",
    "lint-staged": "10.4.2",
    "prettier": "1.18.2",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "tslint-config-airbnb": "5.11.2",
    "tslint-config-prettier": "1.18.0",
    "typedoc": "0.15.0",
    "uuid": "3.3.3"
  },
  "dependencies": {
    "@types/bitcoinjs-lib": "3.4.0",
    "@types/chai": "4.2.3",
    "@types/chai-as-promised": "7.1.2",
    "@types/chai-string": "1.4.2",
    "@types/ethereumjs-abi": "0.6.3",
    "@types/mocha": "5.2.7",
    "@types/node": "^12.7.9",
    "@types/node-fetch": "2.5.2",
    "@types/pdfkit": "0.10.3",
    "@types/qrcode": "1.3.4",
    "@types/sjcl": "1.0.28",
    "@types/uuid": "3.4.5",
    "base64-stream": "1.0.0",
    "bgoldjs-lib": "3.3.3",
    "bignumber.js": "9.0.0",
    "bip69": "2.1.4",
    "bitcoinjs-lib": "3.3.2",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-bignumber": "3.0.0",
    "chai-string": "1.5.0",
    "cross-fetch": "3.0.1",
    "dotenv": "8.1.0",
    "eosjs": "20.0.0",
    "eosjs-ecc": "4.0.7",
    "ethereumjs-abi": "0.6.8",
    "ethereumjs-util": "5.2.0",
    "moment": "2.24.0",
    "pdfkit": "0.10.0",
    "qrcode": "1.4.2",
    "sjcl": "1.0.8",
    "typescript": "^3.6.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
