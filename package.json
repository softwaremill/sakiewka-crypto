{
  "name": "sakiewka-crypto",
  "version": "1.0.0",
  "description": "Client-side Sakiewka app",
  "main": "dist/index.js",
  "engines": {
    "node": ">=10.0.0 <11.0.0",
    "npm": ">=6.0.0 <7.0.0 "
  },
  "scripts": {
    "postinstall": "tsc --skipLibCheck",
    "test-single-currency": "jest --forceExit --verbose false --detectOpenHandles",
    "test": "CURRENCY_UNDER_TEST=BTG npm run test-single-currency && CURRENCY_UNDER_TEST=BTC npm run test-single-currency",
    "watch-test": "npm run test -- --watchAll",
    "build": "tsc",
    "watch": "tsc -w",
    "generate-docs": "typedoc --options ./typedoc.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/softwaremill/sakiewka-client.git"
  },
  "author": "SoftwareMill",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/softwaremill/sakiewka-client/issues"
  },
  "homepage": "https://github.com/softwaremill/sakiewka-client#readme",
  "devDependencies": {
    "concurrently": "3.6.0",
    "jest": "23.6.0",
    "ts-jest": "23.10.5",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "tslint-config-airbnb": "5.11.1",
    "typedoc": "0.11.1",
    "uuid": "3.3.2"
  },
  "dependencies": {
    "@types/bitcoinjs-lib": "3.4.0",
    "@types/chai": "4.1.7",
    "@types/chai-as-promised": "7.1.0",
    "@types/chai-string": "1.4.1",
    "@types/ethereumjs-abi": "0.6.1",
    "@types/mocha": "5.2.7",
    "@types/node": "10.12.12",
